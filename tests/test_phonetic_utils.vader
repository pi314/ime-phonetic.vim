  AssertEqual Testee(['g ', 'hk4']),                   'ㄕ ㄘㄜˋ'
  AssertEqual Testee(['hk4', 'g4', '5j/ ', 'jp6']),    'ㄘㄜˋㄕˋㄓㄨㄥ ㄨㄣˊ'
