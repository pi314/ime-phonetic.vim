Execute ([UT] phonetic_utils#SymbolStr2CodeList()):
  let Testee = function('phonetic_utils#ZhuyinStr2CodeList')
  AssertEqual Testee(''),                         []
  AssertEqual Testee('ㄘㄜˋ'),                    ['hk4']
  AssertEqual Testee('ㄘㄜˋㄕˋ'),                 ['hk4', 'g4']
  AssertEqual Testee('ㄕㄘㄜˋ'),                  ['g', 'hk4']
  AssertEqual Testee('ㄕㄘˋㄜ'),                  ['g', 'h4', 'k']
  AssertEqual Testee('ㄘㄜˋㄕˋㄓㄨㄥ ㄨㄣˊ'),     ['hk4', 'g4', '5j/ ', 'jp6']
  AssertEqual Testee('ㄉㄨ'),                     ['2j']
  AssertEqual Testee('ㄨㄉ'),                     ['j', '2']
  AssertEqual Testee('ㄓㄧ'),                     ['5', 'u']
  AssertEqual Testee('ㄦㄦㄦ'),                   ['-', '-', '-']
  AssertEqual Testee('ㄧㄉㄧㄥㄉㄧㄢˇㄦˊ'),       ['u', '2u/', '2u03', '-6']
  AssertEqual Testee('ㄧㄒㄧㄝ'),                 ['u', 'vu,']
  AssertEqual Testee('ㄓㄨ'),                     ['5j']
  AssertEqual Testee('ˊˇˋ'),                      []
  AssertEqual Testee('ㄓㄨˊˋ'),                   ['5j6']


Execute ([UT] phonetic_utils#CodeList2ZhuyinStr()):
  let Testee = function('phonetic_utils#CodeList2ZhuyinStr')
  AssertEqual Testee([]),                              ''
